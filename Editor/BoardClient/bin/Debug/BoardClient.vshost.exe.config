<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <appSettings>
      <!-- Ип адрес сервера доски по умолчанию -->
      <add key="ipBoard" value="10.3.13.7"/>
      <!--Порт для доски-->
      <add key="Port" value="17777"/>
  
      <!--Параметры удалённого управления-->
      
      <!-- udpIp - Канал поиска клиента 
      ipAddress - ip текущего компьютера -->
      <add key="ipAddress" value="10.3.13.7"/>
      <add key="udpIp" value="10.3.255.255"/>

      <!--Порты для удаленного управления-->
      <add key="imagePort" value="9309"/>
      <add key="controlPort" value="9310"/>
      <add key="lowImagePort" value="9311"/>
      <add key="udpPort" value="9312"/>

    </appSettings>
</configuration>